Host jaco
  User wolf
  HostName jaco.cs.yale.internal
  Port 22
  IdentityFile ~/.ssh/id_rsa.pub
  PreferredAuthentications publickey
  ControlMaster auto
  ControlPath /tmp/%r@%h:%p
  ControlPersist 1h
  ForwardAgent yes

